@model OneCRS.MVC.Models.Sales.Project
<legend style=" margin-left:20px;background-color:#1f497d; padding-left:30px; padding-top:2px; padding-right:50px; padding-bottom:2px; color:white; border-radius:15px; border:3px solid #eeece1; font-size:20px; "><b>Manage Verification & Approval</b></legend>
<br />
<input type="hidden" id="Hide_C_MediumIsSoft" value="@Model.Hide_C_MediumIsSoft" />
<input type="hidden" id="Hide_C_MediumIsToken" value="@Model.Hide_C_MediumIsToken" />
<input type="hidden" id="Hide_C_MediumIsSmart" value="@Model.Hide_C_MediumIsSmart" />
<input type="hidden" id="Hide_C_MediumIsRoaming" value="@Model.Hide_C_MediumIsRoaming" />
<input type="hidden" id="Hide_C_MediumIsServer" value="@Model.Hide_C_MediumIsServer" />

<div class="row">
    <div class="col-lg-6">
        <div class="form-group">
            <label><span style="color: red; font-size: 18px">*</span> List of Media</label>
            <select class="form-control form-white" id="mediaSelect" name="mediaSelect">
                <option value="">Please select..</option>
            </select>
        </div>
    </div>
</div>
<br />
<div class="row" id="roamingverification" style="display:none">
    <div class="col-lg-6">
        <div class="form-group">
            <label class="control-label col-md-4">Verification</label>
            <div class="col-md-8">
                <div id="rdoSelect">
                    <div class="col-md-7">
                        <label>
                            @if (Model.V_IsAP == true)
                            {
                                <input type="radio" value="" name="V_IsAP" id="visap" data-radio="iradio_square-blue" checked="checked" />
                            }
                            else
                            {
                                <input type="radio" value="" name="V_IsAP" id="visap" data-radio="iradio_square-blue" />
                            }
                            AP Verification
                        </label>
                    </div>
                    <div class="col-md-7">
                        <label>
                            @if (Model.V_IsRP == true)
                            {
                                <input type="radio" value="" name="V_IsAP" id="visrp" data-radio="iradio_square-blue" checked="checked" />
                            }
                            else
                            {
                                <input type="radio" value="" name="V_IsAP" id="visrp" data-radio="iradio_square-blue" />
                            }
                            RP Verification
                        </label>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-4">Approval</label>
            <div class="col-md-8">
                <div id="rdoSelect">
                    <div class="col-md-7">
                        <label>
                            @if (Model.A_IsRO == true)
                            {
                                <input type="radio" value="" name="A_IsRO" id="aisro" data-radio="iradio_square-blue" checked="checked" />
                            }
                            else
                            {
                                <input type="radio" value="" name="A_IsRO" id="aisro" data-radio="iradio_square-blue" />
                            }
                            RO Approval
                        </label>
                    </div>
                    <div class="col-md-8">
                        <label>
                            @if (Model.A_IsBypassRO == true)
                            {
                                <input type="radio" value="" name="A_IsRO" id="aisbypass" data-radio="iradio_square-blue" checked="checked" />
                            }
                            else
                            {
                                <input type="radio" value="" name="A_IsRO" id="aisbypass" data-radio="iradio_square-blue" />
                            }
                            Automatic Approval
                        </label>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

