@model OneCRS.MVC.Models.Sales.Project
<legend style=" margin-left:20px;background-color:#1f497d; padding-left:30px; padding-top:2px; padding-right:50px; padding-bottom:2px; color:white; border-radius:15px; border:3px solid #eeece1; font-size:20px; "><b>Manage Document</b></legend>
