@{
    ViewBag.Title = "Manage Data Collection";
    Layout = "~/Views/Shared/_Menu.cshtml";
}

<section>
    <div class="page-content">
        <div class="header">
            <h2><strong>Manage </strong> Default Data Collection (DDC)</h2>
            <div class="breadcrumb-wrapper">
                <ol class="breadcrumb">
                    <li>
                        <a href="../">Dashboard</a>
                    </li>
                    <li class="active">Manage_DDC</li>
                </ol>
            </div>
        </div>
        <div class="panel">
            <div class="panel-content page-wizard">
                <div class="wizard-div current wizard-sea">
                    <form class="form-horizontal" id="Manage_User" role="form">
                        <div class="filtering" style="border: 1px solid #69A8F3;border-radius:4px;padding:10px">
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="SearchRoleID">Search Function</label><br />
                            </div>
                            @*<div class="form-group">
                                <label class="control-label col-sm-1" for="SearchRoleID">Role:</label>
                                <div class="col-sm-6">
                                    <select id="SearchRoleID" name="SearchRoleID" class="form-control form-white" style="width:70%">
                                        <option selected="selected" value="0">All Users</option>
                                    </select>
                                </div>
                            </div>*@
                            <div class="form-group">
                                <label class="control-label col-sm-1" for="searchID">Search:</label>
                                <div class="col-sm-4">
                                    <select id="searchID" name="searchID" class="form-control form-white">
                                        <option selected="selected" value="">Select Action</option>
                                        <option value="Module">Module</option>
                                        <option value="KeyData">Key Data</option>
                                        <option value="Value">Value</option>
                                        <option value="KeyValue">Key Value</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <input type="text" id="findID" class="form-control form-white" />
                                </div>
                                <div class="col-sm-1">
                                    <button type="button" id="btnToFind" class="btn btn-success ladda-button" data-style="expand-right">Search</button>
                                </div>
                            </div>
                        </div>
                        <br />
                        <button type="button" class="btn btn-primary ladda-button" id="btnCreateData"><i class="fa fa-plus"></i> CREATE NEW</button>
                        <div id="DDCJtable">
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <!-- Modal Create -->
        <div class="modal inmodal" id="CreateDDCModal" tabindex="-1" role="dialog" aria-hidden="true" style="display: none">
            <div class="modal-dialog" style="font-size:11px;margin-top: 3%; width: 850px">
                <div class="modal-content">
                    <div class="modal-header bg-primary">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="icons-office-52"></i></button>
                        <h4 class="modal-title"><strong>Create New</strong> Data Collection</h4>
                    </div>
                    <div class="panel">
                        <div class="panel-content page-wizard">
                            <div class="wizard-div current wizard-sea">
                                <form role="form" id="formCreateDDC" class="form-horizontal">
                                    @Html.Partial("~/Views/DefaultDataCollection/_CreateDDCData.cshtml")
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer bg-primary">
                        <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-dark" id="btnSaveCreate">Submit</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- Modal Update-->
        <div class="modal inmodal" id="UpdateDDCModal" tabindex="-1" role="dialog" aria-hidden="true" style="display: none">
            <div class="modal-dialog" style="font-size:11px;margin-top: 3%; width: 850px">
                <div class="modal-content">
                    <div class="modal-header bg-primary">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="icons-office-52"></i></button>
                        <h4 class="modal-title"><strong>Update</strong> Data Collection</h4>
                    </div>
                    <div class="panel">
                        <div class="panel-content page-wizard">
                            <div class="wizard-div current wizard-sea">
                                <form role="form" id="formUpdateDDC" class="form-horizontal">
                                    @Html.Partial("~/Views/DefaultDataCollection/_UpdateDDCData.cshtml")
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer bg-primary">
                        <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-dark" id="btnSaveUpdate">Submit</button>
                    </div>
                </div>
            </div>
        </div>

        <!--View Records -->
        <div class="modal inmodal" id="ViewDDCModal" tabindex="-1" role="dialog" aria-hidden="true" style="display: none">
            <div class="modal-dialog" style="font-size:11px;margin-top: 3%; width: 850px">
                <div class="modal-content">
                    <div class="modal-header bg-primary">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="icons-office-52"></i></button>
                        <h4 class="modal-title"><strong>View Data</strong> Information</h4>
                    </div>
                    <div class="panel">
                        <div class="panel-content page-wizard">
                            <div class="wizard-div current wizard-sea">
                                <form role="form" id="formPackageProcedure" class="form-horizontal">
                                    @Html.Partial("~/Views/DefaultDataCollection/_ViewDDCData.cshtml")
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer bg-primary">
                        <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

