                    <div class="col-md-8">
                        <label>
                            <input type="radio" value="" name="A_IsRO" id="aisbypass" data-parsley-group="block1" data-radio="iradio_square-blue" data-parsley-errors-container='div[id="condition-error"]' required>
                            Automatic Approval
                        </label>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

