<style>
    .datagridupdate table {
        border-collapse: collapse;
        text-align: left;
        width: 100%;
    }

    .datagridupdate {
        font: normal 12px/150% Arial, Helvetica, sans-serif;
        background: #fff;
        overflow: hidden;
        border: 1px solid #8C8C8C;
        -webkit-border-radius: 3px;
        -moz-border-radius: 3px;
        border-radius: 3px;
    }

        .datagridupdate table td, .datagridupdate table th {
            padding: 3px 10px;
        }

        .datagridupdate table thead th {
            background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #8C8C8C), color-stop(1, #7D7D7D) );
            background: -moz-linear-gradient( center top, #8C8C8C 5%, #7D7D7D 100% );
            filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#8C8C8C', endColorstr='#7D7D7D');
            background-color: #8C8C8C;
            color: #FFFFFF;
            font-size: 15px;
            font-weight: bold;
            border-left: 1px solid #A3A3A3;
        }

            .datagridupdate table thead th:first-child {
                border: none;
            }

        .datagridupdate table tbody td {
            color: #7D7D7D;
            border-left: 1px solid #DBDBDB;
            font-size: 12px;
            font-weight: normal;
        }

        .datagridupdate table tbody .alt td {
            background: #EBEBEB;
            color: #7D7D7D;
        }

        .datagridupdate table tbody td:first-child {
            border-left: none;
        }

        .datagridupdate table tbody tr:last-child td {
            border-bottom: none;
        }

        .datagridupdate table tfoot td div {
            border-top: 1px solid #8C8C8C;
            background: #EBEBEB;
        }

        .datagridupdate table tfoot td {
            padding: 0;
            font-size: 12px;
        }

            .datagridupdate table tfoot td div {
                padding: 2px;
            }

            .datagridupdate table tfoot td ul {
                margin: 0;
                padding: 0;
                list-style: none;
                text-align: right;
            }

        .datagridupdate table tfoot li {
            display: inline;
        }

            .datagridupdate table tfoot li a {
                text-decoration: none;
                display: inline-block;
                padding: 2px 8px;
                margin: 1px;
                color: #F5F5F5;
                border: 1px solid #8C8C8C;
                -webkit-border-radius: 3px;
                -moz-border-radius: 3px;
                border-radius: 3px;
                background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #8C8C8C), color-stop(1, #7D7D7D) );
                background: -moz-linear-gradient( center top, #8C8C8C 5%, #7D7D7D 100% );
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#8C8C8C', endColorstr='#7D7D7D');
                background-color: #8C8C8C;
            }

        .datagridupdate table tfoot ul.active, .datagridupdate table tfoot ul a:hover {
            text-decoration: none;
            border-color: #7D7D7D;
            color: #F5F5F5;
            background: none;
            background-color: #8C8C8C;
        }

    div.dhtmlx_window_active, div.dhx_modal_cover_dv {
        position: fixed !important;
    }
</style>

