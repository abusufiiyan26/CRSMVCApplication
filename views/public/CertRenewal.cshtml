<header>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container" style="margin-top:10px;margin-bottom:1px">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a href="~/home">
                    <img src="~/assets/registration/img/freeze/logo.png" style="width:250px" alt="" class="logo">
                </a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            </div>
        </div>
    </nav>
</header>
<br /><br /><br /><br />
<div class="form-container">
    <div class="form-group">
        <div class="alert alert-success media fade in" style="border-radius:7px">
            <p style="color:darkblue">
                <div style="color: darkblue; font-family: 'Lato', Arial"><b>The following is List of Certificate available for renew</b></div>
            </p>
        </div>
    </div>
    <div>
        <table id="renewPublicTbl" style="cursor:pointer"></table>
        <div id="renewPublicFooter"></div>
    </div>
</div>


