@model OneCRS.MVC.Models.Admin.CreateUser
<form id="CreateUserForm">
    <div class="panel" id="personalDetails">
        <div class="panel-content page-wizard">
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Department</label>
                        <select class="form-control form-white" id="Department_ID" name="Department_ID" data-search="true"></select>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Role</label>
                        <select class="form-control form-white" id="Role_ID" name="Role_ID" data-search="true"></select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label>ID Type</label>
                        <select class="form-control form-white" id="IDType" name="IDType" data-placeholder="Select Type...">
                            <option value="IC">IC</option>
                            <option value="PP">Passport</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>IC NO / Passport</label>
                        <input type="text" class="form-control form-white" id="ICNo" name="ICNo" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Name</label>
                        <input type="text" class="form-control form-white" id="Name" name="Name" required data-parsley-group="block0">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Email</label>
                        <input type="text" class="form-control form-white" id="Email" name="Email" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Telephone No</label>
                        <input type="text" class="form-control form-white" id="Mobile_No" name="Mobile_No" required data-parsley-group="block0">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Gender</label>
                        <select class="form-control form-white" id="Gender" name="Gender" data-placeholder="Select Gender...">
                            <option value="M">Male</option>
                            <option value="F">Female</option>
                        </select>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Date of Birth</label>
                        <div class="prepend-icon">
                            <input type="text" id="DOB" class="form-control form-white" name="DOB" required data-parsley-group="block0">
                            <i class="icon-calendar"></i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-8">
                    <div class="form-group">
                        <label>Address</label>
                        <input type="text" class="form-control form-white" id="Address" name="Address" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Country</label>
                        <select class="form-control form-white" id="CountryID" name="CountryID" data-search="true">
                            <option selected value="">Select Country</option>
                        </select>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>State</label>
                        <select class="form-control form-white" id="StateID" name="StateID" data-search="true">
                            <option selected value="">Select State</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Postcode</label>
                        <input type="text" pattern="[0-9]*" maxlength="6"  class="form-control form-white" id="Postcode" name="Postcode" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>City</label>
                        <input type="text" class="form-control form-white" id="City" name="City" required data-parsley-group="block0">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="button1" style="text-align:right">
        <input type="button" class="btn btn-fb btn-embossed" value="NEXT" id="btnNext" />
    </div>
</form>
<form id="CreateAdminForm2">
    <!-- COMPANY INFORMATION -->
    <div class="panel" id="roleDetails" style="display:none">
        <div class="panel-content page-wizard">
            <div class="form-group">
                <div class="col-lg-6">
                    <label>
                        <input type="checkbox" name="is" id="is" data-checkbox="icheckbox_square-blue">
                        Click here for  Organization
                    </label>
                </div>
            </div>
            <br /><br />
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Organization</label>
                        <input type="text" class="form-control form-white" id="OrgName" name="OrgName" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Registration No</label>
                        <input type="text" class="form-control form-white" id="OrgRegNo" name="OrgRegNo" required data-parsley-group="block0">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-8">
                    <div class="form-group">
                        <label>Address</label>
                        <input type="text" class="form-control form-white" id="OrgAddress" name="OrgAddress" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Country</label>
                        <select class="form-control form-white" id="OrgCountryID" name="OrgCountryID" data-search="true"></select>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>State</label>
                        <select class="form-control form-white" id="OrgStateID" name="OrgStateID" data-search="true"></select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Postcode</label>
                        <input type="text" class="form-control form-white" id="OrgPostcode" name="OrgPostcode" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>City</label>
                        <input type="text" class="form-control form-white" id="OrgCity" name="OrgCity" required data-parsley-group="block0">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Fax No</label>
                        <input type="text" class="form-control form-white" id="OrgFaxNo" name="OrgFaxNo" required data-parsley-group="block0">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label>Tel No</label>
                        <input type="text" class="form-control form-white" id="OrgTelNo" name="OrgTelNo" required data-parsley-group="block0">
                    </div>
                </div>
            </div>
            <div class="row" id="hideAPProjectTbl" style="display:none">
                <div class="col-lg-6">
                    <div>
                        <label>Project</label>
                    </div>
                    <div>
                        <select style="width:60%" class="form-control form-white" id="ProjectID" name="ProjectID" data-search="true">
                            <option selected value="">Select Project</option>
                        </select>
                        <button style="width:25%;margin-top:10px" type="button" class="btn btn-primary btn-transparent" id="btnAddProject"><i class="fa fa-plus"></i>ADD</button>
                    </div>
                </div>
                <div class="col-lg-6">
                    <table id="projectGridTbl"></table>
                </div>
            </div>
        </div>
    </div>
</form>
<div id="button2" style="text-align:right;display:none">
    <input type="button" class="btn btn-fb btn-embossed" value="PREV" id="btnPrev" />
    <input type="button" class="btn btn-fb btn-embossed" value="CREATE" data-loading-text="Loading..." id="btnCreate" />
</div>