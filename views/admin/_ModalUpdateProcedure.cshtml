                                    <div class="col-sm-6">
                                        <select style="width:70%" class="form-control form-white" id="UpdateProcedureID" name="UpdateProcedureID"></select>
                                        <button type="button" style="margin-top:8px;margin-left:0px" class="btn btn-sm btn-icon btn-rounded btn-default" id="btnUpdateProcedure"><i class="fa fa-plus"></i></button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer bg-primary">
                <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-dark" id="btnSaveUpdatePackageProcedure">Submit</button>
            </div>
        </div>
    </div>
</div>
