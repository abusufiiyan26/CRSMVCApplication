<header>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header" >
                <a href="~/home">
                    <img src="~/assets/registration/img/freeze/logo.png" style="width:300px" alt="" class="logo">
                </a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

            </div>
        </div>
    </nav>
</header>
<br /><br /><br /><br />
<div id="content">
    <div class="form-container">
        <div id="tmm-form-wizard" class="container substrate">
            <div class="row">
                <div class="col-xs-12">
                    <h2 class="form-login-heading"><span>REVOCATION REQUEST SYSTEM</span></h2>
                </div>
            </div>
            <div id="ConfirmationPnl" class="wizard-step wizard-confirmation">
                <br />
                @Html.Partial("~/Views/Public/_RevokeDetails.cshtml")
                @Html.Partial("~/Views/Public/_RevokeReason.cshtml")
                <div class="wizard-nav">
                    <div class="next">
                        <input class="button button-control" type="button" style="width:100px;height:30px" value="Submit" id="btnPublicRevokeConfirm" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<footer style="background-color:white;text-align:center">
    <div class="container">
        <div class="social">
            @*<img src="~/assets/registration/img/freeze/LogoPOS.jpg" width="283" alt="" class="logo">*@
        </div>
        <div class="rights">
            <p>Copyright &copy; 2015  Sdn Bhd. All right reserved</p>
            <p>1CRS is best viewed on a screen with 1024 x 768 resolution or higher with Internet Explorer 9.0, Chrome, and Safari</p>
        </div>
    </div>
</footer>
@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/assets/registration/js/tmm_form_wizard_custom.js"></script>
    <script src="~/Scripts/SimpleWizardForm.js"></script>
    <script src="~/Scripts/jquery.steps.js"></script>
    <script src="~/Scripts/jquery.steps.min.js"></script>

    <!--Date Picker-->
    <script src="~/assets/global/plugins/jquery-ui/jquery-ui-1.11.2.min.js"></script>
    <script src="~/assets/registration/js/modernizr.custom.32033.js"></script>
    <script src="~/assets/global/js/application.js"></script> <!-- Main Application Script -->
    <script src="~/assets/global/js/plugins.js"></script> <!-- Main Plugin Initialization Script -->
    <script src="~/assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js"></script> <!-- >Bootstrap Date Picker -->
    <!-- Bootstrap-->
    <script src="~/assets/global/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/global/plugins/bootbox/bootbox.min.js"></script>

    <!-- JQRID TABLE-->
    <script src="../Scripts/jquery.jqGrid.min.js"></script>
    <script src="../Scripts/i18n/grid.locale-en.js"></script>

    <!-- File UPLOAD-->
    <script src="../assets/global/plugins/dropzone/dropzone.min.js"></script>

    <!--Landing Page javascript-->
    <script src="~/assets/registration/js/scripts.js"></script>

    <!-- Loader-->
    <script src="../Content/loader/loadingoverlay.min.js"></script>
    
    <script src="../ViewsScript/Public/WizardRevokeSave.js"></script>
}
