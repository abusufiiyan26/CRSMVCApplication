@model OneCRS.MVC.Models.Sales.Project
@{
    ViewBag.Title = "ProjectUpdate";
    Layout = "~/Views/Shared/_Menu.cshtml";
}

<section>
    <div class="page-content">
        <div class="header">
            <h2><strong>Project </strong> Configurations</h2>
            <div class="breadcrumb-wrapper">
                <ol class="breadcrumb">
                    <li>
                        <a href="~/Dashboard">Dashboard</a>
                    </li>
                    <li class="active">Update Project</li>
                </ol>
            </div>
        </div>
        <!-- FIRST STEP HIDDEN VALUES-->
        <input type="hidden" name="hidepaccess_puser" value="" id="hidepaccess_puser" />
        <input type="hidden" name="hidepaccess_pusersi" value="" id="hidepaccess_pusersi" />
        <input type="hidden" name="hidepaccess_ap" value="" id="hidepaccess_ap" />

        <!-- SECOND STEP HIDDEN VALUES-->
        <input type="hidden" name="hideccertcountry" value="" id="hideccertcountry" />
        <input type="hidden" name="hideccertcn" value="" id="hideccertcn" />
        <input type="hidden" name="hideccertsn" value="" id="hideccertsn" />
        <input type="hidden" name="hideccertemail" value="" id="hideccertemail" />
        <input type="hidden" name="hideccertorgname" value="" id="hideccertorgname" />
        <input type="hidden" name="hideccertorgid" value="" id="hideccertorgid" />
        <input type="hidden" name="hidecmediumsoft" value="" id="hidecmediumsoft" />
        <input type="hidden" name="hidecmediumtoken" value="" id="hidecmediumtoken" />
        <input type="hidden" name="hidecmediumsmart" value="" id="hidecmediumsmart" />
        <input type="hidden" name="hidecmediumroaming" value="" id="hidecmediumroaming" />
        <input type="hidden" name="hidecmediumserver" value="" id="hidecmediumserver" />

        <!-- THIRD STEP HIDDEN VALUES-->
        <input type="hidden" name="hideptuserbypass" value="" id="hideptuserbypass" />
        <input type="hidden" name="hideptuseronline" value="" id="hideptuseronline" />
        <input type="hidden" name="hideptusermanual" value="" id="hideptusermanual" />
        <input type="hidden" name="hideptapbypass" value="" id="hideptapbypass" />
        <input type="hidden" name="hideptaponline" value="" id="hideptaponline" />
        <input type="hidden" name="hideptapmanual" value="" id="hideptapmanual" />

        <!-- FOURTH STEP HIDDEN VALUES-->
        <input type="hidden" name="hidesdrequired" value="" id="hidesdrequired" />
        <input type="hidden" name="hidesdic" value="" id="hidesdic" />
        <input type="hidden" name="hidesdssm" value="" id="hidesdssm" />
        <input type="hidden" name="hidesdregslip" value="" id="hidesdregslip" />
        <input type="hidden" name="hidesdauth" value="" id="hidesdauth" />
        <input type="hidden" name="hideSD_IsPassPort" value="" id="hideSD_IsPassPort" />
        <input type="hidden" name="hideSD_IsSupporting1" value="" id="hideSD_IsSupporting1" />
        <input type="hidden" name="hideSD_IsSupporting2" value="" id="hideSD_IsSupporting2" />

        <!-- FIFTH STEP HIDDEN VALUES-->
        <input type="hidden" name="hidevisap" value="" id="hidevisap" />
        <input type="hidden" name="hidevisrp" value="" id="hidevisrp" />
        <input type="hidden" name="hideaisro" value="" id="hideaisro" />
        <input type="hidden" name="hideaisbypass" value="" id="hideaisbypass" />

        <input type="hidden" value="@Model.ErrorResult" id="hideError" name="hideError" />

        <div class="panel">
            <div class="panel-content page-wizard">
                <div class="wizard-div current wizard-sea">
                    <form id="formUpdateProject" class="form-validation">
                        <fieldset style="border-radius: 5px; min-height:150px; border:2px solid #1f497d; background-color:#eeece1;">
                            @Html.Partial("~/Views/Project/_UpdateManageProfile.cshtml")
                        </fieldset>
                        <fieldset style="border-radius: 5px; min-height:150px; border:2px solid #1f497d; background-color:#eeece1;">
                            @Html.Partial("~/Views/Project/_UpdateManageCertificate.cshtml")
                        </fieldset>
                        <fieldset style="border-radius: 5px; min-height:150px; border:2px solid #1f497d; background-color:#eeece1;">
                            @Html.Partial("~/Views/Project/_UpdateManagePayment.cshtml")
                        </fieldset>
                        <fieldset style="border-radius: 5px; min-height:150px; border:2px solid #1f497d; background-color:#eeece1;">
                            @Html.Partial("~/Views/Project/_UpdateManageSupDoc.cshtml")
                        </fieldset>
                        <fieldset style="border-radius: 5px; min-height:150px; border:2px solid #1f497d; background-color:#eeece1;">
                            @Html.Partial("~/Views/Project/_UpdateManageVerfAprv.cshtml")
                        </fieldset>
                    </form>
                    <div class="modal inmodal" id="questionlist" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
                        <div class="modal-dialog" style="font-size:11px;margin-top: 3%; width: 800px">
                            <div class="modal-content">
                                <div class="modal-header bg-blue">
                                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">X</span><span class="sr-only">Close</span></button>
                                    <h4 style="text-align:center" class="modal-title"><strong>MANAGE SECURITY QUESTIONS</strong></h4>
                                </div>
                                <div id="usernamePnl">
                                    <fieldset>
                                        @Html.Partial("~/Views/Project/_UpdateChallengeResponseList.cshtml")
                                    </fieldset>
                                    <div id="button" style="text-align:right">
                                        @*<input type="button" class="btn btn-default btn-embossed" data-dismiss="modal" value="Close">*@
                                        @*<input type="button" class="btn btn-fb btn-embossed" value="RESET" id="btnQuestionReset" />*@
                                        <input type="button" class="btn btn-fb btn-embossed" value="CONFIRM" id="btnQuestionSubmit" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer bg-gray-light">
                    <input type="button" class="btn btn-fb btn-embossed" value="Click HERE to Proceed" id="btnUpdateRedirect" style="display:none"/>
                    <input type="button" class="btn btn-fb btn-embossed" value="Submit" id="btnUpdateSave" data-loading-text="Loading..." />
                </div>
            </div>
        </div>
    </div>
</section>

