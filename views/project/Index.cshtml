@{
    ViewBag.Title = "Manage Project";
    Layout = "~/Views/Shared/_Menu.cshtml";
}

<section>
    <div class="page-content">
        <div class="header">
            <h2><strong>Manage </strong> Project</h2>
            <div class="breadcrumb-wrapper">
                <ol class="breadcrumb">
                    <li>
                        <a href="">Dashboard</a>
                    </li>
                    <li class="active">Manage_Project</li>
                </ol>
            </div>
        </div>
        <div class="panel">
            <div class="panel-content page-wizard">
                <div class="wizard-div current wizard-sea">
                    
                    <form class="form-horizontal" id="Project" role="form">
                        <div class="filtering" style="border: 1px solid #69A8F3;border-radius:4px;padding:10px">
                            <div class="form-group">
                                <label class="control-label col-sm-4" for="SearchRoleID">Search Function</label><br />
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-1" for="Date">Date From:</label>
                                <div class="col-sm-3 prepend-icon">
                                    <input type="text" id="DateFrom" class="form-control form-white" />
                                    <i class="icon-calendar"></i>
                                </div>
                                <label class="control-label col-sm-2" for="Date">Date To:</label>
                                <div class="col-sm-3 prepend-icon">
                                    <input type="text" id="DateTo" class="form-control form-white" />
                                    <i class="icon-calendar"></i>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-1" for="searchID">Search:</label>
                                <div class="col-sm-4">
                                    <select id="searchID" name="searchID" class="form-control form-white">
                                        <option selected="selected" value="">Select Action</option>
                                        <option value="ProjectName">Project Name</option>
                                        <option value="ProjectPrefix">Project Prefix</option>
                                        <option value="ProjectCode">Project Code</option>
                                        <option value="ProjectOwner">Project Owner</option>
                                        <option value="GLCode">GL Code</option>
                                    </select>
                                </div>
                                <div class="col-sm-4">
                                    <input type="text" id="findID" class="form-control form-white" />
                                </div>
                                <div class="col-sm-1">
                                    <button type="button" id="btnToFind" class="btn btn-success ladda-button" data-style="expand-right">Search</button>
                                </div>
                            </div>
                        </div>
                        <br />
                        <input type="button" class="btn btn-fb btn-embossed" value="CREATE NEW PROJECT" id="btnCreateNewProject" />
                        <div id="ProjectGridTbl"></div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

